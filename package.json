  "author": "Guilherme Diego <guilherme.albino.francisco@gmail.com>",
  "license": "MIT",
  "scripts": {
    "script": "ts-node --project tsconfig.scripts.json ./scripts",
    "dev": "yarn script preStart && next dev",
    "build": "next build",
    "start": "yarn script preStart && next start",
    "lint": "eslint './src' './tests'",
    "jest": "node --expose-gc ./node_modules/.bin/jest --config=jest.config.js --logHeapUsage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "pre-push": "yarn tsc"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "yarn lint -- --fix"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/styles": "^4.11.2",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "mongoose": "^5.11.8",
    "mongoose-query-parser": "^1.1.9",
    "mongoose-timestamp": "^0.6.0",
    "next": "^10.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.19",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "cypress": "^6.2.1",
    "cypress-visual-regression": "^1.5.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
